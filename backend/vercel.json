{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"]
      }
    ]
  }
  